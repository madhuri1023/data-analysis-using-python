us='aditya'
password='aditya@123'
user=input('enter user name:')
if us==user:
    n=int(input('enter number:'))
    if n%11==0:
        p=input('enter password:')
        if p==password:
            print('login successfull')
        else:
            print('invalid password')
else:
    print('invalid username')

output:
enter user name:aditya
enter number:121
enter password:aditya@123
login successfull
